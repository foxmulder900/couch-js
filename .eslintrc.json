{
    "env": {
        "jasmine": true,
        "browser": true,
        "node": true
    },
    "extends": "standard",
    "plugins": ["jasmine"],
    "rules": {
        "camelcase": "off",
        "indent": ["error", "tab"],
        "keyword-spacing": ["error", {"before": false, "after": false}],
        "new-cap": "off",
        "no-global-assign": "off",
        "no-return-assign": "off",
        "no-tabs": "off",
        "object-curly-spacing": ["error", "never"],
        "space-before-blocks": ["error", "never"],
        "space-before-function-paren": ["error", "never"],
        "space-infix-ops": "off"
    }
}